{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 400,
    "height": 200,
    "title": {
      "text": "Cyprus - choropleth map",
      "subtitle": ["Identity projection"],
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "subtitlePadding": 5,
      "anchor": "middle",
      "color": "black"
    },
    "data": {
      "url": "https://raw.githubusercontent.com/nikiconst/nikiconst.github.io/main/map_cyprus(5).geojson",
      "format": {"property": "features"}
    },
    "projection": {"type": "identity", "reflectY": true},
    "mark": {"type": "geoshape", "strokeWidth": 0.5},
    "encoding": {
      "color": {"field": "Population(2022)", "type": "quantitative"},
      "tooltip": [
        {"field": "properties.name", "title": " City"},
        {"field": "Population(2022)", "title": "Population"}
      ]
    },
    "config": {},
    "transform": [
      {
        "lookup": "properties.name",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/nikiconst/nikiconst.github.io/main/population%20cyprus.csv"
          },
          "key": "name",
          "fields": ["Population(2022)"]
        }
      }
    ]
  }