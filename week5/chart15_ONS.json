{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://api.allorigins.win/raw?url=https://api.ons.gov.uk/timeseries/JWS3/dataset/LMS/data",
      "format": {"type": "json","property": "years"}
    },
    "title": {
      "text": "Wholesale & retail trade; repair of motor vehicles and motor cycles",
      "subtitle": "United Kingdom (thousands), seasonally adjusted ",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "middle",
      "color": "black"
    },
    "width": 150,
    "height": 150,
    "mark": {"type": "line", "color": "seagreen", "strokeWidth": 2, "opacity": 1},
    "encoding": {
      "x": {"field": "year", "type": "temporal"},
      "y": {"field": "value", "type": "quantitative"}
    }
  }