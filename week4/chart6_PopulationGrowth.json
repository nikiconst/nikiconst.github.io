{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "Population growth (annual %) ",
      "subtitle": "% change in population in the UK",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "data": {
      "url": "https://raw.githubusercontent.com/nikiconst/nikiconst.github.io/main/population_growth(UK).csv"
    },
    "height": 300,
    "width": 260,
    "mark": {"type": "bar", "color": "#86d0bb"},
    "encoding": {
      "x": {
        "field": "Year",
        "type": "temporal",
        "axis": {"title": null, "grid": false, "ticks": false, "labelAngle": 45}
      },
      "y": {
        "field": "Population growth(annual%)",
        "type": "quantitative",
        "title": "",
        "axis": {"grid": false}
      }
    },
    "config": {}
  }