{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Change in UK population(annual %) ",
    "subtitle": "% change in population in the UK",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "middle",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/nikiconst/nikiconst.github.io/main/week4/PopulationGrowth(UK).csv"
  },
  "height": 300,
  "width": 260,
  "mark": {"type": "bar", "color": "#86d0bb"},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "axis": {"title": null, "grid": false, "ticks": false, "labelAngle": 45}
    },
    "y": {
      "field": "Population growth(annual%)",
      "type": "quantitative",
      "title": "Population growth(%)",
      "axis": {"grid": false}
    }
  },
  "config": {}
}  